    <div class="d-flex justify-content-between">
        <h3 class="text-primary">Clãs</h3>
        <a class="btn btn-primary" onclick="ShowModalEdit()"><i class="bi-plus"></i></a>
    </div>
    <% if(Clan && Clan.length> 0){%>
    <table class="table table-dark table-striped mt-2">
        <thead>
            <th>Id</th>
            <th>Nome</th>
            <th>Descricao</th>
            <th>Ações</th>
        </thead>
        <tbody>
            <% Clan.forEach(element=> {%>
                <tr>
                    <td>
                        <%= element.Id %>
                    </td>
                    <td id='item_name_<%= element.Id %>'>
                        <%= element.Nome %>
                    </td>
                    <td>
                        <%= element.Descricao %>
                    </td>

                    <td>
                        <i class="bi-pen btn text-primary" onclick='ShowModalEdit("<%= element.Id %>")'></i>
                        <i onclick='ShowModalDeleteItem("<%= element.Id  %>")'
                            class="bi-trash text-danger btn"></i>
                    </td>
                </tr>
                <% }); %>
        </tbody>
    </table>
    <% }else{%>
        <p>Nenhum clã cadastrado!</p>
    <% } %>


<%- include('partials/ModalEdit', {rota:"Clan", action:"/InserirClan"}); %>
<%- include('partials/ModalDeleteItem',{rota: "Clan" , action:"/DeleteClan"}); %>
<style>
    .container-img {
    box-shadow: 0px 0px 5px black;
}
</style>